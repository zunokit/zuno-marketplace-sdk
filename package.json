{
  "name": "zuno-marketplace-sdk",
  "version": "2.0.0-beta-claude.3",
  "description": "All-in-One NFT Marketplace SDK with Wagmi & React Query built-in",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    },
    "./exchange": {
      "types": "./dist/exchange/index.d.ts",
      "import": "./dist/exchange/index.mjs",
      "require": "./dist/exchange/index.js"
    },
    "./auction": {
      "types": "./dist/auction/index.d.ts",
      "import": "./dist/auction/index.mjs",
      "require": "./dist/auction/index.js"
    },
    "./collection": {
      "types": "./dist/collection/index.d.ts",
      "import": "./dist/collection/index.mjs",
      "require": "./dist/collection/index.js"
    },
    "./logger": {
      "types": "./dist/logger/index.d.ts",
      "import": "./dist/logger/index.mjs",
      "require": "./dist/logger/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.mjs",
      "require": "./dist/testing/index.js"
    },
    "./devtools": {
      "types": "./dist/devtools/index.d.ts",
      "import": "./dist/devtools/index.mjs",
      "require": "./dist/devtools/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "examples/**/*"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest --config tests/setup/jest.config.js",
    "test:watch": "jest --config tests/setup/jest.config.js --watch",
    "test:coverage": "jest --config tests/setup/jest.config.js --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "nft",
    "marketplace",
    "web3",
    "ethereum",
    "wagmi",
    "react-query",
    "sdk",
    "blockchain",
    "smart-contracts"
  ],
  "author": "Zuno Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZunoKit/zuno-marketplace-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ZunoKit/zuno-marketplace-sdk/issues"
  },
  "homepage": "https://github.com/ZunoKit/zuno-marketplace-sdk#readme",
  "dependencies": {
    "@tanstack/query-core": "^5.59.0",
    "@tanstack/react-query": "^5.59.0",
    "@types/jest": "^30.0.0",
    "@wagmi/connectors": "^5.1.0",
    "@wagmi/core": "^2.13.0",
    "axios": "^1.7.0",
    "ethers": "^6.13.0",
    "viem": "^2.21.0",
    "wagmi": "^2.12.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rimraf": "^6.0.0",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.2.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
